{

  "manifest_version": 2,
  "name": "Osu! New maps",
  "version": "1.2",

  "description": "List of new maps Osu!",
  "icons": {
    "48": "icon.png"
  },
  
  "browser_action": {
	 "default_popup": "index.html"
  },
  
  "permissions": ["https://osu.ppy.sh/home"],
  
  "browser_specific_settings": {
    "gecko": {
      "id": "OsuNewMapsExtension@github.com",
      "strict_min_version": "60.0"
    }
  },

  "content_scripts": [
    {
      "matches": ["https://osu.ppy.sh/home"],
      "js": ["index.js"]
    }
  ]

}